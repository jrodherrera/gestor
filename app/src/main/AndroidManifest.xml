<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dam.migestor">
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Base.MiGestor">
        <activity android:name=".ui.terms.TermsActivity"></activity>
        <activity android:name=".ui.ThanksActivity" />
        <activity android:name=".ui.OkDocActivity" />
        <activity android:name=".ui.ErrorDocActivity" />
        <activity android:name=".ui.TransferActivity" />
        <activity android:name=".ui.OperationsActivity" />
        <activity android:name=".ui.instructions.InstructionsActivity" />
        <activity android:name=".ui.HomeActivity" />
        <activity android:name=".ui.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>